bundler-help () {
  echo "Bundler Aliases"
  echo
  echo " b     = bundle"
  echo " bi    = bundle install --path vendor"
  echo " be    = bundle exec"
  echo " bl    = bundle list"
  echo " bu    = bundle update"
  echo " bp    = bundle package"
  echo
}